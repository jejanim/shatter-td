        set udg_int_creepnr[i] = 0
        set i = i + 1
    endloop

    set udg_int_lifes = 0
    set i = 0
    loop
        exitwhen (i > 8)
        set udg_int_spawn[i] = 0
        set i = i + 1
    endloop

    set udg_int_circleint = 0
    set i = 0
    loop
        exitwhen (i > 8)
        set udg_unitgrp_tempcreep[i] = CreateGroup()
        set i = i + 1
    endloop

    set udg_time_choosing = CreateTimer()
    set udg_timer_creeps = CreateTimer()
    set udg_dia_gamemode = DialogCreate()
    set i = 0
    loop
        exitwhen (i > 8)
        set udg_int_block[i] = 0
        set i = i + 1
    endloop

    set udg_temp_int = 0
    set udg_timer_mode = CreateTimer()
    set udg_temp_unitgrp = CreateGroup()
    set i = 0
    loop
        exitwhen (i > 8)
        set udg_int_hero_unspent_levels[i] = 0
        set i = i + 1
    endloop

    set udg_str_health = ""
    set udg_str_mana = ""
    set udg_str_board_char = ""
    set udg_bool_changed = false
endfunction

