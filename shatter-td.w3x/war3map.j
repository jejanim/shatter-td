function Trig_creeps_announce_Actions takes nothing returns nothing
    call PlaySoundBJ( gg_snd_GoodJob )
    set udg_int_wave = ( udg_int_wave + 1 )
    call TimerDialogSetTitleBJ( udg_timerwindow_creeps, ( "Next wave (" + ( I2S(( udg_int_wave + 1 )) + "):" ) ) )
    call CreateNUnitsAtLoc( 1, udg_unittype_creep[udg_int_wave], Player(PLAYER_NEUTRAL_PASSIVE), udg_point_creepcreate, bj_UNIT_FACING )
    set udg_unit_tempcreep = GetLastCreatedUnit()
    call DisplayTextToForce( GetPlayersAll(), ( ( "Wave " + I2S(udg_int_wave) ) + " incoming." ) )
    call DisplayTextToForce( GetPlayersAll(), "TRIGSTR_3895" )
    call DisplayTextToForce( GetPlayersAll(), ( I2S(udg_int_creepnr[udg_int_wave]) + "x" ) )
    call DisplayTextToForce( GetPlayersAll(), GetUnitName(udg_unit_tempcreep) )
    call DisplayTextToForce( GetPlayersAll(), ( I2S(R2I(GetUnitStateSwap(UNIT_STATE_LIFE, udg_unit_tempcreep))) + " health" ) )
    call RemoveUnit( udg_unit_tempcreep )
    call ConditionalTriggerExecute( gg_trg_creeps_spawn )
endfunction

