function InitTrig_creeps_spawn takes nothing returns nothing
    set gg_trg_creeps_spawn = CreateTrigger(  )
    call TriggerAddAction( gg_trg_creeps_spawn, function Trig_creeps_spawn_Actions )
endfunction

//===========================================================================
// Trigger: creeps move
//===========================================================================
function Trig_creeps_move_Func002Func002Func001A takes nothing returns nothing
    set udg_temp_unit = GetEnumUnit()
    call GroupRemoveUnitSimple( udg_temp_unit, udg_unitgrp_tempcreep[GetForLoopIndexB()] )
    // relocate
    call SetUnitPositionLocFacingBJ( udg_temp_unit, udg_point_spawn[GetForLoopIndexB()], 90.00 )
endfunction

function Trig_creeps_move_Actions takes nothing returns nothing
    call PlaySoundBJ( udg_sound_wave[udg_int_wave] )
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = udg_int_creepnr[udg_int_wave]
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call TriggerSleepAction( 0.50 )
        set bj_forLoopBIndex = 1
        set bj_forLoopBIndexEnd = 8
        loop
            exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
            call ForGroupBJ( GetRandomSubGroup(1, udg_unitgrp_tempcreep[GetForLoopIndexB()]), function Trig_creeps_move_Func002Func002Func001A )
            set bj_forLoopBIndex = bj_forLoopBIndex + 1
        endloop
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
endfunction

