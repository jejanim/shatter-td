//===========================================================================
// Trigger: defeat
//===========================================================================
function Trig_defeat_Conditions takes nothing returns boolean
    if ( not ( GetDyingUnit() == udg_unit_queen ) ) then
        return false
    endif
    return true
endfunction

function Trig_defeat_Actions takes nothing returns nothing
    call PauseAllUnitsBJ( true )
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 4
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call PanCameraToTimedLocForPlayer( ConvertedPlayer(GetForLoopIndexA()), GetRectCenter(gg_rct_queen), 0 )
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    call CinematicModeBJ( true, GetPlayersAll() )
    call TriggerSleepAction( 5.00 )
    call ConditionalTriggerExecute( gg_trg_health_update )
    call CinematicModeBJ( false, GetPlayersAll() )
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 4
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CustomDefeatBJ( ConvertedPlayer(GetForLoopIndexA()), "TRIGSTR_3898" )
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
endfunction

