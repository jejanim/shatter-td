//===========================================================================
// Trigger: var player
//===========================================================================
function Trig_var_player_Actions takes nothing returns nothing
    // temp
    call SetPlayerFlagBJ( PLAYER_STATE_GIVES_BOUNTY, true, Player(8) )
    call SetPlayerFlagBJ( PLAYER_STATE_GIVES_BOUNTY, true, Player(9) )
    // temp
    set udg_region_tower_area[1] = gg_rct_tower_area_p1
    set udg_region_tower_area[2] = gg_rct_tower_area_p2
    set udg_region_tower_area[3] = gg_rct_tower_area_p3
    set udg_region_tower_area[4] = gg_rct_tower_area_p4
    set udg_region_tower_area[5] = gg_rct_tower_area_p5
    set udg_region_tower_area[6] = gg_rct_tower_area_p6
    set udg_region_tower_area[7] = gg_rct_tower_area_p7
    set udg_region_tower_area[8] = gg_rct_tower_area_p8
    // temp
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 8
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateFogModifierRectBJ( true, ConvertedPlayer(GetForLoopIndexA()), FOG_OF_WAR_MASKED, gg_rct_creep_create_1 )
        call CreateFogModifierRectBJ( true, ConvertedPlayer(GetForLoopIndexA()), FOG_OF_WAR_MASKED, gg_rct_creep_create_2 )
        call CreateFogModifierRectBJ( true, ConvertedPlayer(GetForLoopIndexA()), FOG_OF_WAR_MASKED, gg_rct_creep_create_3 )
        call CreateFogModifierRectBJ( true, ConvertedPlayer(GetForLoopIndexA()), FOG_OF_WAR_MASKED, gg_rct_creep_create_4 )
        // 0 = Player didn't select a summoner yet.
        set udg_int_spawn[GetForLoopIndexA()] = 0
        // Player properties and supply.
        call SetPlayerStateBJ( ConvertedPlayer(GetForLoopIndexA()), PLAYER_STATE_RESOURCE_LUMBER, 1 )
        call SetPlayerStateBJ( ConvertedPlayer(GetForLoopIndexA()), PLAYER_STATE_FOOD_CAP_CEILING, 1 )
        call SetPlayerStateBJ( ConvertedPlayer(GetForLoopIndexA()), PLAYER_STATE_RESOURCE_FOOD_CAP, 1 )
        call SetPlayerStateBJ( ConvertedPlayer(GetForLoopIndexA()), PLAYER_STATE_RESOURCE_GOLD, 100 )
        call CreateFogModifierRectBJ( true, ConvertedPlayer(GetForLoopIndexA()), FOG_OF_WAR_VISIBLE, GetPlayableMapRect() )
        call CreateFogModifierRectBJ( true, ConvertedPlayer(GetForLoopIndexA()), FOG_OF_WAR_MASKED, gg_rct_creep_create_1 )
        call FogMaskEnableOn(  )
        // Visibility and intro
        call CameraSetupApplyForPlayer( true, gg_cam_Throne_front, ConvertedPlayer(GetForLoopIndexA()), 0 )
        call SetUserControlForceOff( GetPlayersAll() )
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    // temp
    call ConditionalTriggerExecute( gg_trg_var_upgrades )
    call ConditionalTriggerExecute( gg_trg_var_sound )
    call DisableTrigger( GetTriggeringTrigger() )
endfunction

