//===========================================================================
function Trig_creeps_spawn_Func001001001001 takes nothing returns boolean
    return ( GetPlayerSlotState(GetFilterPlayer()) == PLAYER_SLOT_STATE_PLAYING )
endfunction

function Trig_creeps_spawn_Func001001001002 takes nothing returns boolean
    return ( GetPlayerController(GetFilterPlayer()) == MAP_CONTROL_USER )
endfunction

function Trig_creeps_spawn_Func001001001 takes nothing returns boolean
    return GetBooleanAnd( Trig_creeps_spawn_Func001001001001(), Trig_creeps_spawn_Func001001001002() )
endfunction

function Trig_creeps_spawn_Func001Func002C takes nothing returns boolean
    if ( not ( ModuloInteger(GetConvertedPlayerId(udg_temp_player), 2) == 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_creeps_spawn_Func001Func006A takes nothing returns nothing
    // cast abilities
    call IssueTargetOrderBJ( GetEnumUnit(), "frostarmor", GetEnumUnit() )
    call IssueTargetOrderBJ( GetEnumUnit(), "banish", GetEnumUnit() )
    call IssueTargetOrderBJ( GetEnumUnit(), "antimagicshell", GetEnumUnit() )
    call IssueTargetOrderBJ( GetEnumUnit(), "rejuvination", GetEnumUnit() )
    call IssueTargetOrderBJ( GetEnumUnit(), "innerfire", GetEnumUnit() )
    call IssueImmediateOrderBJ( GetEnumUnit(), "defend" )
    call IssueImmediateOrderBJ( GetEnumUnit(), "divineshield" )
    call IssueImmediateOrderBJ( GetEnumUnit(), "manashieldon" )
    call IssueTargetOrderBJ( GetEnumUnit(), "spiritlink", GetEnumUnit() )
    call IssueTargetOrderBJ( GetEnumUnit(), "bloodlust", GetEnumUnit() )
    call IssueTargetOrderBJ( GetEnumUnit(), "doom", GetEnumUnit() )
    call IssueImmediateOrderBJ( GetEnumUnit(), "phaseshifton" )
endfunction

function Trig_creeps_spawn_Func001A takes nothing returns nothing
    set udg_temp_player = GetEnumPlayer()
    if ( Trig_creeps_spawn_Func001Func002C() ) then
        set udg_player_spawner = Player(9)
    else
        set udg_player_spawner = Player(8)
    endif
    call CreateNUnitsAtLoc( udg_int_creepnr[udg_int_wave], udg_unittype_creep[udg_int_wave], udg_player_spawner, udg_point_creepcreate, bj_UNIT_FACING )
    set udg_temp_unitgrp = GetLastCreatedGroup()
    call GroupAddGroup( udg_temp_unitgrp, udg_unitgrp_tempcreep[GetConvertedPlayerId(udg_temp_player)] )
    call ForGroupBJ( udg_temp_unitgrp, function Trig_creeps_spawn_Func001Func006A )
endfunction

function Trig_creeps_spawn_Actions takes nothing returns nothing
    call ForForce( GetPlayersMatching(Condition(function Trig_creeps_spawn_Func001001001)), function Trig_creeps_spawn_Func001A )
    call TriggerSleepAction( 3.00 )
    call ConditionalTriggerExecute( gg_trg_creeps_move )
endfunction

